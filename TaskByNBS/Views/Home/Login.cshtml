
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <link href="https://fonts.googleapis.com/css2?family=Nunito+Sans&display=swap" rel="stylesheet" type="text/css" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.0/css/all.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/CSS/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/CSS/bootstrap.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/CSS/custom-css.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/CSS/sweetalert.css" rel="stylesheet" type="text/css" />
    <script src="~/Content/JS/bootstrap.min.js" type="text/javascript"></script>
    <meta name="viewport" content="width=device-width" />
    <title> Log!n </title>

</head>
<body>
    <div class="container-fluid">
        <div class="row">

            <div class="col-xs-12 col-sm-12 col-md-1 col-lg-1 col-xl-1"></div>

            <div class="col-xs-12 col-sm-12 col-md-10 col-lg-10 col-xl-10">

                <div class="card-body mt-5">

                    <div class="row">
                        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 col-xl-12">
                            <h3 class="h3txtClass"> Log!n </h3>
                        </div>
                    </div>

                    

                    <form action="javascript:void(0)" method="post" enctype="multipart/form-data" onsubmit="return onLoginForm(event, this);">
                        <div class="row mt-4">
                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-6 col-xl-6">
                                <div class="form-group">
                                    <label> Username ( Email / Phone ) </label>
                                    <i class="far fa-user fascustomUserCSS"></i>
                                    <input type="text" name="UserName" value="" id="UserName" class="form-control" />
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-6 col-xl-6">
                                
                            </div>
                        </div>
                        <div class="row mt-4">
                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-6 col-xl-6">
                                <div class="form-group">
                                    <label> Password </label>
                                    <i class="fas fa-wallet fascustomSalaryCSS"></i>
                                    <input type="text" name="UserPassword" value="" id="UserPassword" class="form-control" />
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-6 col-xl-6">
                                
                            </div>
                        </div>

                        <div class="row mt-4">
                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-6 col-xl-6">
                                
                            </div>
                            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-6 col-xl-6">
                                <div class="row">
                                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-6 col-xl-6">
                                        <div class="form-group">
                                            <button class="btn customBtn" type="submit" id="BtnSubmit"> Log!n </button>
                                        </div>
                                    </div>
                                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-6 col-xl-6">
                                        <div class="form-group">
                                           
                                        </div>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </form>
                </div>

            </div>

            <div class="col-xs-12 col-sm-12 col-md-1 col-lg-1 col-xl-1"></div>
        </div>
    </div>
    <script src="~/Content/JS/jquery-1.10.2.min.js" type="text/javascript"></script>
    <script src="~/Content/JS/sweetalert.min.js" type="text/javascript"></script>
    <script src="~/Content/JS/custom-js.js" type="text/javascript"></script>
    <script src="~/Content/JS/validate-js.js" type="text/javascript"></script>
</body>
</html>

